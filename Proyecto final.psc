// Funcion anti mike mejorada
funcion anti_mike_2(pikmin,enemigo,hp_1,hp_2,ene_1,ene_2,dato)
	largo = Longitud(dato)
	contador_verificador = 0
	para a = 1 Hasta largo Con Paso 1 Hacer
		posicion = Subcadena(dato,a,a)
		si (posicion == "1") o (posicion == "2") o (posicion == "3") o (posicion == "4") o (posicion == "5") o (posicion == "6") o (posicion == "7") o (posicion == "8") o (posicion == "9") Entonces
			contador_verificador = contador_verificador + 1
		FinSi
	FinPara
	si contador_verificador <> largo Entonces
		Borrar Pantalla
		interfaz_pikmin(enemigo,hp_1,hp_2,ene_1,ene_2)
		interfaz_pikmin(pikmin,hp_1,hp_2,ene_1,ene_2)
